{
  "name": "react-native-elements",
  "version": "1.0.0-beta",
  "description": "React Native Elements & UI Toolkit",
  "main": "src/index.js",
  "types": "src/index.d.ts",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/dabit3/React-Native-Elements.git"
  },
  "keywords": [
    "react-native",
    "reactjs",
    "reactnative",
    "bootstrap"
  ],
  "scripts": {
    "build": "rm -rf build; webpack",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "postinstall": "opencollective postinstall",
    "lint": "eslint src/",
    "precommit": "lint-staged && npm test",
    "prettify": "prettier --write 'src/**/*.js'"
  },
  "author": "Nader Dabit & Monte Thakkar",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/react-native-training/react-native-elements/issues"
  },
  "homepage": "https://github.com/react-native-training/react-native-elements#readme",
  "collective": {
    "type": "opencollective",
    "url": "https://opencollective.com/react-native-elements",
    "logo": "https://opencollective.com/react-native-elements/logo.txt"
  },
  "dependencies": {
    "lodash.isempty": "^4.4.0",
    "lodash.times": "^4.3.2",
    "opencollective": "^1.0.3",
    "prop-types": "^15.5.8"
  },
  "devDependencies": {
    "babel": "^6.23.0",
    "babel-core": "^6.23.1",
    "babel-eslint": "^8.2.1",
    "babel-jest": "^22.1.0",
    "babel-loader": "^7.1.2",
    "babel-preset-react-native": "^4.0.0",
    "codecov": "^3.0.0",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "enzyme-to-json": "^3.3.0",
    "eslint": "^4.16.0",
    "eslint-plugin-react": "^7.5.1",
    "eslint-plugin-react-native": "^3.2.1",
    "file-loader": "^1.1.6",
    "husky": "^0.14.3",
    "image-webpack-loader": "^3.3.1",
    "jest": "^22.1.4",
    "lint-staged": "^6.0.1",
    "prettier": "^1.7.4",
    "react": "^16.2.0",
    "react-addons-test-utils": "^15.6.0",
    "react-dom": "^16.2.0",
    "react-native": "^0.52.0",
    "react-native-vector-icons": "^4.4.2",
    "react-test-renderer": "^16.2.0",
    "webpack": "^3.10.0"
  },
  "peerDependencies": {
    "react-native-vector-icons": "^4.2.0"
  },
  "jest": {
    "preset": "react-native",
    "coverageDirectory": "./coverage/",
    "collectCoverageFrom": [
      "src/**/*.js",
      "!src/index.js",
      "!src/helpers/*.js"
    ],
    "collectCoverage": true,
    "globals": {
      "__DEV__": true
    },
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/assetsTransformer.js",
      "\\.(css|less)$": "<rootDir>/assetsTransformer.js"
    }
  },
  "lint-staged": {
    "src/**/*.js": [
      "prettier --single-quote --trailing-comma es5 --write",
      "git add"
    ]
  }
}
